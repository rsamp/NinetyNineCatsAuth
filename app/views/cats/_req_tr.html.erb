<tr>
  <td><%= User.where(id: req.user_id).user_name %></td>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
      <% if req.pending? && !Cat.where(user_id: current_user.id, id: @cat.id).empty? %>
          <%= button_to "Approve", approve_cat_rental_request_url(req) %>
          <%= button_to "Deny", deny_cat_rental_request_url(req) %>
      <% else %>
        <%= req.status %>
      <% end %>
  </td>
</tr>
